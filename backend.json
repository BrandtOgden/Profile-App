{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "patternProperties": {
      "^[A-Za-z0-9]+$": {
        "type": "object",
        "properties": {
          "age": { "type": "string" },
          "gender": { "type": "string" },
          "image": { "type": "string", "format": "uri" },
          "name": { "type": "string" },
          "occupation": { "type": "string" }
        },
        "required": ["age", "gender", "image", "name", "occupation"],
        "additionalProperties": false
      }
    }
}
  